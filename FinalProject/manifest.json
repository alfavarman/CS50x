{
    "name": "Productivity Blocker",
    "version": "1.0.2",
    "manifest_version": 3,
    "description": "A Chrome extension that allows users to block access to certain URLs based on a predefined list.",
    "icons": {
      "16": "./icons/active16.png",
      "32": "./icons/active32.png",
      "48": "./icons/active48.png",
      "128": "./icons/active128.png"
    },
  
    "permissions": [
      "declarativeNetRequest",
      "storage",
      "contextMenus"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "options.html",
      "default_icon": {
        "16": "./icons/active16.png",
        "32": "./icons/active32.png"
      }
    }
  }
